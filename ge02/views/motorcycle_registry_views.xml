<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="motorcycle_registry_tree_inherit_stock_lot" model="ir.ui.view">
        <field name="name">motorcycle.registry.tree.inherit.stock.lot</field>
        <field name="model">motorcycle.registry</field>
        <field name="inherit_id" ref="motorcycle_registry.motorcycle_registry_view_list" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='registry_number']" position="after">
                <field name="lot_id" />
            </xpath>
        </field>
    </record>
</odoo>